---
pageable: true
---
<%-
  current_page.data.title = if defined?(month)
    "#{month} #{year} archive"
  else
    "#{year} archive"
  end
-%>

<% wrap_layout :layout do %>

  <% page_articles.reverse.each do |post| %>
    <%= partial "_post_content", locals: {current_article: post} %>
  <% end %>

  <%= partial "_pagination" %>

<% end %>
